<%= form_with(model: [:admin, @test, @question], local: true) do |form| %>
  <%= render 'shared/validation_errors', resource: @question %>
  <div class='form-group'>
  <%= form.label :body %>
  <%= form.text_field :body, id: :question_body, class: 'form-control', placeholder: Question.human_attribute_name(:body) %>
  </div>
  <%= form.submit class: 'btn btn-primary btn-block' %>
<% end %>
